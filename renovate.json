{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "automerge": true,
  "automergeType": "pr",
  "automergeStrategy": "squash",
  "requiredStatusChecks": [
    "Auto Merge / check / Build and Test",
    "Auto Merge / lint / Code Style"
  ],
  "rebaseWhen": "conflicted"
}
