<?xml version="1.0" encoding="utf-8"?><!--
   Sample data extraction rules file; uncomment and customize as necessary.
   See https://developer.android.com/about/versions/12/backup-restore#xml-changes
   for details.
-->
<data-extraction-rules>
    <cloud-backup>
        <include
            domain="sharedpref"
            path="user_preferences.xml" />
        <include
            domain="database"
            path="vocabri.db" />
        <include
            domain="file"
            path="." />
        <exclude
            domain="file"
            path="cache/" />
    </cloud-backup>
    <device-transfer>
        <include
            domain="sharedpref"
            path="user_preferences.xml" />
        <include
            domain="database"
            path="vocabri.db" />
        <include
            domain="file"
            path="." />
        <exclude
            domain="file"
            path="temp_files/" />
    </device-transfer>
</data-extraction-rules>
